{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "node": true
    },
    "rules": {
        "semi": "error",
        "no-console": 1,
        "no-unused-vars": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-empty": 1,
        "no-extra-parens": 1,
        "no-extra-semi": 1,
        "no-irregular-whitespace": 1,
        "no-constant-condition": 1,
        "no-cond-assign": 1
    }
}